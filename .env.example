# 환경변수 설정 파일 (.env)
# 이 파일은 .gitignore에 추가해야 합니다!

# JWT 설정
SECRET_KEY=your-super-secret-jwt-key-change-this-in-production-12345
ALGORITHM=HS256

# 데이터베이스 설정
# 개발용 (SQLite)
DATABASE_URL=sqlite:///./lotto_app.db

# 프로덕션용 예시 (PostgreSQL)
# DATABASE_URL=postgresql://username:password@localhost:5432/lotto_db

# 카카오 OAuth 설정
# 카카오 개발자 콘솔에서 발급받은 키로 변경해야 합니다
KAKAO_CLIENT_ID=your-kakao-app-key
KAKAO_CLIENT_SECRET=your-kakao-secret-key
KAKAO_REDIRECT_URI=http://localhost:8000/auth/kakao/callback

# 서버 설정
HOST=0.0.0.0
PORT=8000
DEBUG=true